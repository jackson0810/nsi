<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule platformId="wdpVpr" name="admin.n-s-i.us virtual path rewrite" stopProcessing="true">
                    <match url="^.*$" ignoreCase="false" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="^admin\.n-s-i\.us$" />
                    </conditions>
                    <action type="Rewrite" url="/admin/{R:0}" appendQueryString="true" />
                </rule>
                <rule platformId="wdpVpr" name="m2.n-s-i.us virtual path rewrite" stopProcessing="true">
                    <match url="^.*$" ignoreCase="false" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="^m2\.n-s-i\.us$" />
                    </conditions>
                    <action type="Rewrite" url="/nsiDraft2/{R:0}" appendQueryString="true" />
                </rule>
            </rules>
            <outboundRules>
                <rule name="admin.n-s-i.us virtual path rewrite">
                    <match serverVariable="RESPONSE_LOCATION" pattern="^(https?\://admin\.n-s-i\.us)/admin/(.+/)$" />
                    <conditions>
                        <add input="{RESPONSE_STATUS}" pattern="^301$" ignoreCase="false" />
                    </conditions>
                    <action type="Rewrite" value="{R:1}/{R:2}" />
                </rule>
                <rule name="m2.n-s-i.us virtual path rewrite">
                    <match serverVariable="RESPONSE_LOCATION" pattern="^(https?\://m2\.n-s-i\.us)/nsiDraft2/(.+/)$" />
                    <conditions>
                        <add input="{RESPONSE_STATUS}" pattern="^301$" ignoreCase="false" />
                    </conditions>
                    <action type="Rewrite" value="{R:1}/{R:2}" />
                </rule>
            </outboundRules>
        </rewrite>
        <handlers accessPolicy="Read, Execute, Script">
        </handlers>
<httpErrors errorMode="Detailed" />
    </system.webServer>
</configuration>
